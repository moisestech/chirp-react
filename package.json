{
  "name": "chirp-react",
  "version": "1.0.0",
  "description": "Chirp - public social messaging application built in ReactJS",
  "main": "index.js",
  "scripts": {
    "start": "webpack-dev-server",
    "build": "NODE_ENV='production' webpack"
  },
  "babel": {
    "presets": [
      "@babel/preset-env",
      [
        "@babel/preset-react",
        {
          "runtime": "automatic"
        }
      ]
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties",
      "syntax-dynamic-import"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/moisestech/chirp-react.git"
  },
  "keywords": [
    "ReactJS",
    "Hooks",
    "Twitter"
  ],
  "author": "Moises Sanabria",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/moisestech/chirp-react/issues"
  },
  "homepage": "https://github.com/moisestech/chirp-react#readme",
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.9.1",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "firebase": "^7.22.1",
    "firebase-tools": "^8.12.0",
    "prop-types": "^15.7.2",
    "react": "0.0.0-experimental-4ead6b530",
    "react-dom": "^17.0.0-rc.3",
    "react-flip-move": "^3.0.4",
    "react-redux": "^7.2.1",
    "react-redux-loading-bar": "^5.0.0",
    "react-router-dom": "^5.2.0",
    "react-twitter-embed": "^3.0.3",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.11.6",
    "@babel/plugin-proposal-class-properties": "^7.10.4",
    "@babel/plugin-transform-react-jsx": "^7.10.4",
    "@babel/preset-env": "^7.11.5",
    "@babel/preset-react": "^7.10.4",
    "babel-loader": "^8.1.0",
    "copy-webpack-plugin": "^6.2.0",
    "css-loader": "^4.3.0",
    "html-webpack-plugin": "^4.5.0",
    "style-loader": "^1.3.0",
    "webpack": "^4.44.2",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
